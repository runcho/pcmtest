<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog title="{i18n_Static>starterDetailsPage.dialog.facetUpdate.fragment.title}" type="Message">
		<content>
			<l:Grid defaultSpan="L12 M12 S12" width="auto">
				<l:content>
					<f:SimpleForm id="Questionnaire1_UpdateForm" minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout"
						labelSpanS="4" labelSpanL="1" labelSpanM="2" emptySpanL="2" emptySpanM="2" columnsL="2" columnsS="2" columnsM="2">
						<f:content>
							<VBox>
								<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
								<Label required="false" text="{i18n>CHP.Questionnaire1.UI.LineItem.UI.DataField.Answer}"></Label>
								<ComboBox enabled="true" value="{/Questionnaire1Answer/Value}" name="Answer" change="handleComboChange"
									items="{path:'/Questionnaire1Answer'}" id="Answer_662"><core:Item key="{Code}" text="{Description}"/></ComboBox>
							</VBox>
						</f:content>
					</f:SimpleForm>
				</l:content>
			</l:Grid>
		</content>
		<beginButton>
			<Button class="saveButton" text="{i18n_Static>starterDetailsPage.dialog.facetUpdate.fragment.Save.btn}" press="confirmUpdate"/>
		</beginButton>
		<endButton>
			<Button class="cancelButton" text="{i18n_Static>starterDetailsPage.dialog.facetAdd.fragment.cancel.btn}" press="handleExit"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>