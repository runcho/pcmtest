<mvc:View height="100%" controllerName="c4c.create-PartnerCollection.remote.view.create" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns="sap.m">
	<Page
		title="{path: 'i18n_Static>starterCreateTemplate.create.view.title,CHP.Partner.UI.HeaderInfo.TypeName.Name', formatter:'.formatTranslation'}"
		class="marginBoxContent">
		<content>
			<f:SimpleForm id="PartnerCollection_Form" minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" labelSpanS="4"
				labelSpanL="1" labelSpanM="2" emptySpanL="2" emptySpanM="2" columnsL="2" columnsS="2" columnsM="2">
				<f:content>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label required="false" text="{i18n>CHP.Partner.UI.Identification.UI.DataField.Name}"></Label>
						<Input enabled="true" value="{/Name}" id="Name_346"></Input>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label required="false" text="{i18n>CHP.Partner.UI.Identification.UI.DataField.AdditionalName}"></Label>
						<Input enabled="true" value="{/AdditionalName}" id="AdditionalName_347"></Input>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label required="false" text="{i18n>CHP.Partner.UI.Identification.UI.DataField.DUNS}"></Label>
						<Input enabled="true" value="{/DUNS}" id="DUNS_348"></Input>
					</VBox>
				</f:content>
			</f:SimpleForm>
		</content>
		<footer >
			<Toolbar class="footer">
				<Button text="{i18n_Static>starterCreateTemplate.create.view.reset.btn}" press='resetForm'/>
				<ToolbarSpacer/>
				<Button class="saveButton" text="{i18n_Static>starterCreateTemplate.create.view.save.btn}" press='saveForm'/>
				<Button class="cancelButton" text="{i18n_Static>starterCreateTemplate.create.view.cancel.btn}" press='cancelForm'/>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>