<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog title="{i18n_Static>starterDetailsPage.dialog.facetUpdate.fragment.title}" type="Message">
		<content>
			<l:Grid defaultSpan="L12 M12 S12" width="auto">
				<l:content>
					<f:SimpleForm id="Contact_UpdateForm" minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" labelSpanS="4"
						labelSpanL="1" labelSpanM="2" emptySpanL="2" emptySpanM="2" columnsL="2" columnsS="2" columnsM="2">
						<f:content>
							<VBox>
								<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
								<Label required="false" text="{i18n>CHP.Contact.UI.LineItem.UI.DataField.FirstName}"></Label>
								<Input enabled="true" value="{/FirstName}" id="FirstName_687"></Input>
							</VBox>
							<VBox>
								<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
								<Label required="false" text="{i18n>CHP.Contact.UI.LineItem.UI.DataField.LastName}"></Label>
								<Input enabled="true" value="{/LastName}" id="LastName_688"></Input>
							</VBox>
							<VBox>
								<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
								<Label required="false" text="{i18n>CHP.Contact.UI.LineItem.UI.DataField.Mobile}"></Label>
								<Input enabled="true" value="{/Mobile}" id="Mobile_689"></Input>
							</VBox>
							<VBox>
								<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
								<Label required="false" text="{i18n>CHP.Contact.UI.LineItem.UI.DataField.Phone}"></Label>
								<Input enabled="true" value="{/Phone}" id="Phone_690"></Input>
							</VBox>
							<VBox>
								<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
								<Label required="false" text="{i18n>CHP.Contact.UI.LineItem.UI.DataField.Email}"></Label>
								<Input enabled="true" value="{/Email}" id="Email_691"></Input>
							</VBox>
						</f:content>
					</f:SimpleForm>
				</l:content>
			</l:Grid>
		</content>
		<beginButton>
			<Button class="saveButton" text="{i18n_Static>starterDetailsPage.dialog.facetUpdate.fragment.Save.btn}" press="confirmUpdate"/>
		</beginButton>
		<endButton>
			<Button class="cancelButton" text="{i18n_Static>starterDetailsPage.dialog.facetAdd.fragment.cancel.btn}" press="handleExit"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>