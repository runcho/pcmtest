<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<l:Grid defaultSpan="L12 M12 S12" width="auto">
		<l:content>
			<f:SimpleForm minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3"
				emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1">
				<f:content>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.PartnerContact.UI.Identification.UI.DataField.EmployeeID}"></Label>
						<Text text="{ObjectPageModel>/PartnerContact/EmployeeID}" tooltip="{ObjectPageModel>/PartnerContact/EmployeeID}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.PartnerContact.UI.Identification.UI.DataField.FirstName}"></Label>
						<Text text="{ObjectPageModel>/PartnerContact/FirstName}" tooltip="{ObjectPageModel>/PartnerContact/FirstName}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.PartnerContact.UI.Identification.UI.DataField.MiddleName}"></Label>
						<Text text="{ObjectPageModel>/PartnerContact/MiddleName}" tooltip="{ObjectPageModel>/PartnerContact/MiddleName}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.PartnerContact.UI.Identification.UI.DataField.LastName}"></Label>
						<Text text="{ObjectPageModel>/PartnerContact/LastName}" tooltip="{ObjectPageModel>/PartnerContact/LastName}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.PartnerContact.UI.Identification.UI.DataField.StatusText}"></Label>
						<Text text="{ObjectPageModel>/PartnerContact/StatusText}" tooltip="{ObjectPageModel>/PartnerContact/StatusText}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.PartnerContact.UI.Identification.UI.DataField.Department}"></Label>
						<Text text="{ObjectPageModel>/PartnerContact/Department}" tooltip="{ObjectPageModel>/PartnerContact/Department}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.PartnerContact.UI.Identification.UI.DataField.Function}"></Label>
						<Text text="{ObjectPageModel>/PartnerContact/Function}" tooltip="{ObjectPageModel>/PartnerContact/Function}"></Text>
					</VBox>
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>