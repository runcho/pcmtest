<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<l:Grid defaultSpan="L12 M12 S12" width="auto">
		<l:content>
			<f:SimpleForm minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3"
				emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1">
				<f:content>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.Appointment.UI.Identification.UI.DataField.SubjectName}"></Label>
						<Text text="{ObjectPageModel>/Appointment/SubjectName}" tooltip="{ObjectPageModel>/Appointment/SubjectName}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.Appointment.UI.Identification.UI.DataField.Account}"></Label>
						<Text text="{ObjectPageModel>/Appointment/Account}" tooltip="{ObjectPageModel>/Appointment/Account}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.Appointment.UI.Identification.UI.DataField.AccountID}"></Label>
						<Text text="{ObjectPageModel>/Appointment/AccountID}" tooltip="{ObjectPageModel>/Appointment/AccountID}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.Appointment.UI.Identification.UI.DataField.Location}"></Label>
						<Text text="{ObjectPageModel>/Appointment/Location}" tooltip="{ObjectPageModel>/Appointment/Location}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.Appointment.UI.Identification.UI.DataField.AppointmentStartDateTime}"></Label>
						<Text text="{path: 'ObjectPageModel>/Appointment/AppointmentStartDateTime', formatter:'.formatTimeDate'}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.Appointment.UI.Identification.UI.DataField.AppointmentEndDateTime}"></Label>
						<Text text="{path: 'ObjectPageModel>/Appointment/AppointmentEndDateTime', formatter:'.formatTimeDate'}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.Appointment.UI.Identification.UI.DataField.Category}"></Label>
						<Text text="{ObjectPageModel>/Appointment/CategoryText}" tooltip="{ObjectPageModel>/Appointment/CategoryText}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.Appointment.UI.Identification.UI.DataField.Priority}"></Label>
						<Text text="{ObjectPageModel>/Appointment/PriorityText}" tooltip="{ObjectPageModel>/Appointment/PriorityText}"></Text>
					</VBox>
					<VBox>
						<layoutData><l:GridData span="L4 M6 S12"/></layoutData>
						<Label text="{i18n>CHP.Appointment.UI.Identification.UI.DataField.StatusText}"></Label>
						<Text text="{ObjectPageModel>/Appointment/StatusText}" tooltip="{ObjectPageModel>/Appointment/StatusText}"></Text>
					</VBox>
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>