{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "hcp://pcmcpapps/PartnerRegistration/manifest.json",
	  	"i18n": "i18n/messageBundle.properties",
		"type": "application",
	  	"title": "{{page.title}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"resources": {
			"css": [{
				"uri": "stylesheet.css"
			}]
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/messageBundle.properties"
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": false
		},
		"componentName": "c4c.registration.local",
		"dependencies": {
			"minUI5Version": "1.36.0",
			"libs": {
				"sap.ui.commons": {
					"minVersion": "1.36.0"
				},
				"sap.m": {
					"minVersion": "1.36.0"
				}
			}
		}
	}
}
